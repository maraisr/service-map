{
	"presets": [
		"env",
		"react"
	],
	"plugins": [
		"transform-object-rest-spread",
		"transform-react-constant-elements",
		[
			"transform-runtime",
			{
				"helpers": false,
				"polyfill": false,
				"regenerator": true
			}
		],
		[
			"transform-react-jsx",
			{
				"pragma": "require('react').createElement"
			}
		]
	]
}
